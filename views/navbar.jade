doctype html
html(lang="en")

  head
    meta(charset="UTF-8")
    meta(name="viewport", content="width=device-width, initial-scale=1.0")
    link(href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/css/bootstrap.min.css", rel="stylesheet", integrity="sha384-9ndCyUaIbzAi2FUVXJi0CjmCapSmO7SnpJef0486qhLnuZ2cdeRhO02iuK6FUUVM", crossorigin="anonymous")

  body
   header.p-3.text-bg-dark(style="height:90px")
    .container
      .d-flex.flex-wrap.align-items-center.justify-content-center.justify-content-lg-start
        ul.nav.col-12.col-lg-auto.me-lg-auto.mb-2.justify-content-center.mb-md-0
          li
            button.nav-link.px-2.text-white(onclick="window.history.back()") Back
          li
            a.nav-link.px-2.text-white(href="/startpage") Home
          li
            a.nav-link.px-2.text-white(href="/profile/viewProfile") Profile
          
          li
            a.nav-link.px-2.text-white(href="/list/candidates") Candidates
      
        form.col-12.col-lg-auto.mb-3.mb-lg-0.me-lg-3(role="search")
          input.form-control.form-control-dark.text-bg-dark(type="search" placeholder="Search..." aria-label="Search")
      
        div.text-end
          button.btn.btn-outline-light.me-2(type="button" onclick="window.location.href = '/'") Login
          button.btn.btn-warning(type="button" onclick="window.location.href = '/register'") Create new acount

       